		"ipad",
		"tiapp",
		"xml",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"ignore": true
	}
